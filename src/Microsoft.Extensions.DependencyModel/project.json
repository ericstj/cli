{
    "description": "Abstractions for reading `.deps` files.",
    "version": "1.0.0-*",
    "repository": {
        "type": "git",
        "url": "git://github.com/dotnet/cli"
    },
    "compilationOptions": {
        "warningsAsErrors": true,
        "keyFile": "../../tools/Key.snk"
    },
    "dependencies": {
        "Newtonsoft.Json": "7.0.1",
        "Microsoft.NETCore.Platforms": "1.0.1-rc2-23704"
    },
    "frameworks": {
        "net451": { },
        "dotnet5.4": {
            "imports": "portable-net45+win8",
            "dependencies": {
                "System.IO.FileSystem": "4.0.1-rc2-23704",
                "System.Linq": "4.0.1-rc2-23704",
                "System.Runtime": "4.0.21-rc2-23704",
                "System.Reflection": "4.1.0-rc2-23704",
                "System.Dynamic.Runtime": "4.0.11-rc2-23704"
            }
        }
    },
    "scripts": {
        "postcompile": [
            "../../scripts/build/place-binary \"%compile:OutputDir%/%project:Name%.dll\"",
            "../../scripts/build/place-binary \"%compile:OutputDir%/%project:Name%.pdb\""
        ]
    }
}